{
    "extends": "standard",
    "plugins": [
        "standard",
        "promise"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": ["error", "always"],
        "no-useless-escape": 0,
        "quotes": ["error", "double"],
        "key-spacing": 0,
        "indent": ["error", 4]
    }
}